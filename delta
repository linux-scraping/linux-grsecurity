--	movl $1,%eax
--	ret
--verify_cpu_sse_ok:
--	popfl				# Restore caller passed flags
--	xorl %eax, %eax
--	ret
-diff --git a/arch/x86/kernel/vm86_32.c b/arch/x86/kernel/vm86_32.c
